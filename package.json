{
    "name": "date-picker",
    "version": "1.0.0",
    "description": "This is tiny date picker library for npm",
    "main": "build/index.js",
    "module": "build/index.es.js",
    "files": [
        "build"
    ],
    "author": "Vitaliy Rudakovskiy",
    "license": "MIT",
    "scripts": {
        "build": "rollup -c",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "test": "jest",
        "prettier": "prettier --write src/**/*.{js,ts,tsx}",
        "format": "prettier . --write",
        "lint": "eslint --ignore-path .eslintignore \"src/**/*.{js,ts,tsx}\" --fix --max-warnings=0",
        "prepare": "husky install"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{ts,tsx,html,css}": "yarn format",
        "src/**/*.{tsx,ts}": "eslint"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.4",
        "@babel/core": "^7.23.7",
        "@babel/preset-env": "^7.23.8",
        "@babel/preset-typescript": "^7.23.3",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@storybook/addon-essentials": "7.6.8",
        "@storybook/addon-interactions": "7.6.8",
        "@storybook/addon-links": "7.6.8",
        "@storybook/addon-onboarding": "1.0.10",
        "@storybook/blocks": "7.6.8",
        "@storybook/react": "7.6.8",
        "@storybook/react-webpack5": "7.6.8",
        "@storybook/test": "7.6.8",
        "@testing-library/jest-dom": "^6.2.0",
        "@testing-library/react": "^14.1.2",
        "@types/jest": "^29.5.11",
        "@types/react": "^18.2.48",
        "@typescript-eslint/eslint-plugin": "^6.19.0",
        "@typescript-eslint/parser": "^6.19.0",
        "babel-core": "^6.26.3",
        "babel-plugin-module-resolver": "^5.0.0",
        "babel-plugin-source-map-support": "^2.2.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-promise": "^6.1.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "prettier": "^3.2.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rollup": "^4.9.5",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "storybook": "7.6.8",
        "ts-jest": "^29.1.1",
        "typescript": "^5.3.3"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "dependencies": {
        "husky": "^8.0.3"
    }
}
